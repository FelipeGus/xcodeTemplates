<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>View Model and a View Controller.</string>
	<key>Summary</key>
	<string>Creates a View Model file, together with it&apos;s view controller. And the integration needed to the module data Manager.</string>
	<key>SortOrder</key>
	<string>30</string>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>DefaultCompletionName</key>
	<string>File</string>
	<key>MainTemplateFile</key>
	<string>___FILEBASENAME___.swift</string>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>productName</string>
			<key>Description</key>
			<string>The name of the VM and VC being created.</string>
			<key>Type</key>
			<string>text</string>
			<key>Name</key>
			<string>Name:</string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Identifier</key>
			<string>MODULENAME</string>
			<key>Description</key>
			<string>The name of the model which these files are part from. </string>
			<key>Type</key>
			<string>text</string>
			<key>Name</key>
			<string>Module name: </string>
			<key>Required</key>
			<true/>
			<key>NotPersisted</key>
			<true/>
		</dict>
	</array>
</dict>
</plist>
